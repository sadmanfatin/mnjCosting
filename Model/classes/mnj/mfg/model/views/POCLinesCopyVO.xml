  <SQLQuery>
    <![CDATA[select distinct 
       mmph.HEADER_NO POC_NO,
       mmph.STYLE_NAME_NEW,
       mmph.STYLE_NO,
       mmph.SEASON,
        mmph.AGENCY_COMMISSION,
        mmph.FABRIC_FINANCE,
          mmph.TRIM_FINANCE,
       mmpl.LINE_ID,
       mmpl.HEADER_NUMBER SUBCOSTING_NO, 
       mmpl.FABRIC_DESC,
       mmpl.FABRIC_CONTENT,
     
       mmpl.WASH_NAME, 
       mmpl.COLOUR,   
       mmpl.CHILD_STYLE_NO,
       mmpl.FABRIC_COST, 
       mmpl.TRIM_COST, 
       mmpl.WASH_COST, 
       mmpl.OTHERS,
       mmpl.CM, 
       mmpl.FOB_WITH_COMM,      
       mmpl.PROD_PER_LINE_PER_H,
       mmpl.STYLE_EFFICIENCY,
       mmpl.SAM,
       mmpl.SHIPMENT,
       mmpl.SHIPMENT_VALUE,
       mmpl.CM_MERCHAND
from MNJ_MFG_PRECOSTING_L mmpl,MNJ_MFG_PRECOSTING_H mmph 

where 
    NVL(CLONE_FLAG,'N') <> 'Y'               
    and  mmph.HEADER_ID = mmpl.HEADER_ID
    and mmph.VERSION_NO = (select max(VERSION_NO) from MNJ_MFG_PRECOSTING_H h where mmph.HEADER_NO = h.HEADER_NO Group by h.HEADER_NO)
ORDER BY "POC_NO","SUBCOSTING_NO"]]>
  </SQLQuery>
